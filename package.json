{
  "name": "islamic-calendar-authority",
  "version": "1.0.0",
  "description": "Comprehensive Islamic calendar library with prayer times, Ramadan dates, and Qibla calculation based on official religious authorities worldwide",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./prayer": {
      "import": "./src/prayer/index.js"
    },
    "./qibla": {
      "import": "./src/qibla/index.js"
    }
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "islamic",
    "calendar",
    "prayer-times",
    "namaz",
    "salat",
    "ramadan",
    "qibla",
    "hijri",
    "muslim",
    "islam",
    "fajr",
    "dhuhr",
    "asr",
    "maghrib",
    "isha",
    "diyanet",
    "isna",
    "mwl",
    "umm-al-qura"
  ],
  "author": "Ramazan App Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/islamic-calendar-authority.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/islamic-calendar-authority/issues"
  },
  "homepage": "https://github.com/your-username/islamic-calendar-authority#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "index.js",
    "index.cjs",
    "core.js",
    "methods.js",
    "data/",
    "src/",
    "utils/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0"
  }
}
